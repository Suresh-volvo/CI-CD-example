pipeline {
    agent any
    
    stages {
        stage ('Checkout') {

            steps {
                    
                echo 'checkout' 
            }
        }

       stage ('Compile Stage') {

            steps {
                
                    echo 'Compile '
                
            }
        }

        stage ('Testing Stage') {

            steps {
                
                    echo ' test'
                
            }
        }


        stage ('Deployment Stage') {
            steps {
               
                    echo ' deploy'
                
            }
        }
    }
}
